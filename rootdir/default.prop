#
# Default properties
#

# Insecure boot
ro.secure=0
ro.allow.mock.location=0
persist.mtk.aee.aed=on
# Enable adb root
ro.debuggable=1
ro.adb.secure=0
persist.sys.usb.config=mass_storage,adb
persist.service.acm.enable=0
# Change to SDCARD to boot from sdcard
ro.mount.fs=EXT4
ro.persist.partition.support=no
# Start adbd on boot
xdvs.adbd.enabled=1
ro.build.type=userdebug
# Allow root access
persist.sys.root_access=1